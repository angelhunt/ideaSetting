<keymap version="1" name="Default copy 0" parent="$default">
  <action id="EditorBackSpace">
    <keyboard-shortcut first-keystroke="back_space" />
    <keyboard-shortcut first-keystroke="shift back_space" />
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
    <keyboard-shortcut first-keystroke="ctrl alt 1" />
  </action>
</keymap>